{
  "serviceName": "RasterRendering",
  "type": "ImageServer",
  "description": "The RasterRendering service is a multi-tenant raster rendering provider.",
  "capabilities": "Image,Catalog,Mensuration,Metadata",
  "provider": "ArcObjects",
  "clusterName": "default",
  "minInstancesPerNode": 0,
  "maxInstancesPerNode": 2,
  "instancesPerContainer": 1,
  "maxWaitTime": 600,
  "maxStartupTime": 300,
  "maxIdleTime": 180,
  "maxUsageTime": 600,
  "loadBalancing": "ROUND_ROBIN",
  "isolationLevel": "HIGH",
  "configuredState": "STOPPED",
  "recycleInterval": 24,
  "recycleStartTime": "00:00",
  "keepAliveInterval": -1,
  "private": false,
  "isDefault": true,
  "maxUploadFileSize": 0,
  "allowedUploadFileTypes": "",
  "properties": {
    "outputDir": "${arcgisoutput}",
    "defaultCompressionTolerance": "0.01",
    "maxScale": "0",
    "allowFunction": "true",
    "maxImageWidth": "15000",
    "copyRight": "",
    "rasterFunctions": "",
    "supportedImageReturnTypes": "MIME+URL",
    "defaultCompressionQuality": "75",
    "allowedMensurationCapabilities": "Basic,Base-Top Height,Top-Top Shadow Height,Base-Top Shadow Height,3D",
    "isCached": "false",
    "virtualOutputDir": "/rest/directories/arcgisoutput",
    "path": "@",
    "maxImageHeight": "4100",
    "cacheControlMaxAge": "43200",
    "colormapToRGB": "false",
    "allowedItemMetadata": "Full",
    "allowedCompressions": "None,JPEG,LZ77,LERC",
    "defaultResamplingMethod": "1",
    "minScale": "0"
  },
  "extensions": [
    {
      "typeName": "WCSServer",
      "capabilities": null,
      "enabled": "false",
      "maxUploadFileSize": 0,
      "allowedUploadFileTypes": "",
      "properties": {}
    },
    {
      "typeName": "WMSServer",
      "capabilities": null,
      "enabled": "false",
      "maxUploadFileSize": 0,
      "allowedUploadFileTypes": "",
      "properties": {}
    },
    {
      "typeName": "JPIPServer",
      "capabilities": null,
      "enabled": "false",
      "maxUploadFileSize": 0,
      "allowedUploadFileTypes": "",
      "properties": {}
    }
  ],
  "frameworkProperties": {},
  "datasets": []
}